+
+  const formatMoscowDate = (value) => {
+    const parsed = new Date(value);
+    if (Number.isNaN(parsed.getTime())) return value;
+    const formatted = parsed.toLocaleString('sv-SE', {
+      timeZone: 'Europe/Moscow',
+      hour12: false,
+    });
+    return `${formatted}+03`;
+  };
+
+  const formatValue = (key, value) => {
+    if (value === null || value === undefined) return 'NULL';
+    if (typeof value !== 'string') return value;
+
+    const isDateField = /(?:_dt|_date)$/i.test(key);
+    if (!isDateField) return value;
+
+    return formatMoscowDate(value);
+  };
+
+  let table = '<table class="db-results-table"><thead><tr>';
+  const headers = Object.keys(data[0] || {});
